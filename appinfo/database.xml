<?xml version="1.0" encoding="UTF-8" ?>
<database>
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>true</overwrite>
	<charset>utf8</charset>
	<table>
		<name>*dbprefix*maps_favorites</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<autoincrement>1</autoincrement>
				<length>41</length>
			</field>
			<field>
				<name>user_id</name>
				<type>text</type>
				<length>64</length>
			</field>
			<field>
				<name>name</name>
				<type>text</type>
				<length>64</length>
			</field>
			<field>
				<name>timestamp</name>
				<type>text</type>
				<length>64</length>
			</field>
			<field>
				<name>lat</name>
				<type>text</type>
				<length>64</length>
			</field>
			<field>
				<name>lng</name>
				<type>text</type>
				<length>64</length>
			</field>
		</declaration>
	</table>
	<table>
		<name>*dbprefix*maps_apikeys</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<autoincrement>1</autoincrement>
				<length>41</length>
			</field>
			<field>
				<name>user_id</name>
				<type>text</type>
				<length>64</length>
			</field>
			<field>
				<name>api_key</name>
				<type>text</type>
				<length>64</length>
			</field>
		</declaration>
	</table>
</database>
